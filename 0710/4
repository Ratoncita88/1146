    <head>
        <meta charset="utf-8">
        
        <title>Arrays </title>
        
        <script>

	let source = [1, 3, 9, 3, 12, 16, 5, 2, 0, 9];
	
	function main(source){
		let max = source[0]; 
		let nextmax = null;
		for (let i = 0; i < source.length; i++) { 
			if (max < source[i]) {
			nextMax = max;
			max = source[i]; 
			}
    
			else if(max != source[i] && nextMax < source[i]){
			nextMax = source[i];
			}
		}
  console.log(max);
  console.log(nextMax);
}
 main(source);		


	</script>
    </head>
    <body>
        
    </body>
</html>